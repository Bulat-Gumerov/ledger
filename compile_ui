#!/usr/bin/bash
uic -g python -o ./ui_main_window.py ./UI/main_window.ui
uic -g python -o ./ui_rebuild_window.py ./UI/rebuild_window.ui
uic -g python -o ./ui_account_choice_dlg.py ./UI/account_choice_dlg.ui
uic -g python -o ./ui_active_choice_dlg.py ./UI/active_choice_dlg.ui
uic -g python -o ./ui_peer_choice_dlg.py ./UI/peer_choice_dlg.ui
uic -g python -o ./ui_account_type_dlg.py ./UI/account_type_dlg.ui

sed -i 's/QString()/""/g' ./ui_main_window.py
sed -i 's/QUrl, Qt)/QUrl, Qt, QDate)/' ./ui_main_window.py
sed -i 's/import AccountSelector/from CustomUI.account_select import AccountSelector/' ./ui_main_window.py
sed -i 's/import AccountButton/from CustomUI.account_select import AccountButton/' ./ui_main_window.py
sed -i 's/import ActiveSelector/from CustomUI.active_select import ActiveSelector/' ./ui_main_window.py
sed -i 's/import PeerSelector/from CustomUI.peer_select import PeerSelector/' ./ui_main_window.py
